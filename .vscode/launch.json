    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'test_gui_rust_2'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=test_gui_rust_2",
                    "--package=test_gui_rust_2"
                ],
                "filter": {
                    "name": "test_gui_rust_2",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "request": "launch",
            "name": "Launch executable 'test_gui_rust_2'",
            "command": "cargo run --bin test_gui_rust_2 --package test_gui_rust_2",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'test_gui_rust_2'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=test_gui_rust_2",
                    "--package=test_gui_rust_2"
                ],
                "filter": {
                    "name": "test_gui_rust_2",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}